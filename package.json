{
  "name": "morkdown",
  "version": "3.0.0-pre01",
  "description": "A markdown editor built on Chrome & Node",
  "main": "lib/server.js",
  "dependencies": {
    "after": "~0.8.1",
    "become": "~1.4.0",
    "bl": "~0.9.1",
    "browserify": "~5.11.1",
    "brucedown": "~1.0.2",
    "codemirror": "~4.5.0",
    "concat-stream": "~1.4.6",
    "delayed": "~1.0.1",
    "domready": "~1.0.6",
    "he": "~0.5.0",
    "hyperquest": "~0.3.0",
    "optimist": "~0.6.1",
    "shoe": "0.0.15",
    "st": "~0.5.1",
    "through": "~2.3.4"
  },
  "bin": {
    "morkdown": "./bin/morkdown.js"
  },
  "keywords": [
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rvagg/morkdown.git"
  },
  "preferGlobal": true,
  "authors": [
    "Rod Vagg <rod@vagg.org> (https://github.com/rvagg)",
    "Ian Duffy <ian@ianduffy.ie> (https://github.com/imduffy15)",
    "ralphtheninja <ralphtheninja@riseup.net> (https://github.com/ralphtheninja)"
  ],
  "scripts": {
    "install": "mkdir -p static/codemirror/; cp -a node_modules/codemirror/lib/codemirror.css node_modules/codemirror/theme/ static/codemirror/"
  },
